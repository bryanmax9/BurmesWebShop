{
  "expo": {
    "name": "Burmes",
    "slug": "ecommerce-web",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/BurmesLogo.ico",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/BurmesLogo.ico",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": { "supportsTablet": true },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/BurmesLogo.ico",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/BurmesLogo.ico",
      "bundler": "metro"
    }
  }
}
